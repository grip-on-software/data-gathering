{
    "$id": "https://gros.liacs.nl/schema/data-gathering/vcs/github_update.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "GitHub update tracker",
    "$ref": "#/$defs/github_update",
    "$defs": {
        "github_update": {
            "type": "object",
            "patternProperties": {
                ".+": {
                    "$ref": "../utils.json#/$defs/datetime",
                    "description": "Moment when GitHub auxiliary data of the repository is most recently collected."
                }
            }
        }
    }
}
