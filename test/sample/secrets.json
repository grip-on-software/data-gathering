{
    "salts": {
        "salt": "salttoken",
        "pepper": "peppertoken"
    },
    "usernames": [
        {
            "prefix": "AD\\",
            "mutate": "lower"
        },
        {
            "prefix": "%@domain.test",
            "pattern": "(\\w+)@domain\\.test",
            "replace": "$1"
        }
    ]
}
